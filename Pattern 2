import java.util.*;
class pat
{
 public static void main(String[] args) 
{
 try
 {
 Scanner sc= new Scanner(System.in);
 System.out.print("Enter a number: ");
 int c= sc.nextInt();
 for (int i=c; i>=1;i--)
 {
  for(int j=1;j<=i;j++)
  {
      System.out.print(i);
      
      
  }
  
  System.out.println();
  
 }
 c=c-1;
 }
 catch(Exception e)
 {
   System.out.println("Due to character exception");
 }
}
}
